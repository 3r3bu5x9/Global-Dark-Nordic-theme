!xrdb -merge ~/.Xresources!
!SCrollbar!
!URxvt.background      : #111217!
URxvt.scrollBar: false
URxvt.internalBorder:10

!font!
URxvt.font:     xft:Source Code Variable:size=9:antialias=true
URxvt.boldFont: xft:Source Code Variable:Bold:size=9:antialias=true
URxvt.letterSpace: -1

! special
URxvt.foreground      : #0088CC
URxvt.foreground_bold : #17A4D6
URxvt.cursorColor     : #0088CC
URxvt.background      : #073642

! black
URxvt.color0:       #073642
URxvt.color8:       #191919

! red
URxvt.color1:       #E36464
URxvt.color9:       #E36464

! green
URxvt.color2  : #FF4B12
URxvt.color10 : #CB4B16

! yellow
URxvt.color3:       #aec795
